{
    "buildCommand": "npm install && npm run build --workspace=client",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "installCommand": "npm install",
    "devCommand": "npm run dev --workspace=client",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}